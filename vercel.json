{
  "buildCommand": "prisma generate && next build",
  "devCommand": "next dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "src/pages/api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "regions": ["hkg1"],
  "crons": [
    {
      "path": "/api/cron/sync-all",
      "schedule": "0 * * * *"
    }
  ]
}
